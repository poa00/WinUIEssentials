<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
    <metadata>
        <id>WinUIEssential.WinUI3</id>
        <version>1.0.14</version>
        <title>WinUIEssential(WinUI3)</title>
        <authors>Peter</authors>
        <owners></owners>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <description>Essential helpers for developing WinUI(UWP)</description>
        <icon>assets\MainLogo.png</icon>
        <dependencies>
            <group targetFramework="native"/>
        </dependencies>
    </metadata>
    <files>
        <!--build/native-->
        <file src="include/Glyphs.hpp" target="build/native/include"/>
        <file src="include/ToastBuilder.hpp" target="build/native/include"/>
        <file src="include/ToastTemplates.hpp" target="build/native/include"/>
        <file src="include/PropertyChangeHelper.hpp" target="build/native/include"/>
        <file src="include/BadgeGlyphs.hpp" target="build/native/include"/>
        <file src="include/Taskbar.hpp" target="build/native/include"/>
        <file src="include/TemplateControlHelper.hpp" target="build/native/include"/>
        <file src="WinUIEssential.WinUI3.targets" target="build/native"/>
        
        <!--lib/native-->
        <file src="..\x64\Debug\WinUI3Package\WinUI3Package.winmd" target="lib/native"/>
        <file src="..\x64\Debug\WinUI3Package\WinUI3Package.dll" target="lib/native"/>

        <!--control resources-->
        <file src="..\x64\Debug\WinUI3Package\WinUI3Package\*.*" target="lib/native"/>

        <file src="..\MainLogo.png" target="assets\"/>
    </files>
</package>